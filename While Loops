n = input()
def sum_even_odd(n):

    even = 0
    odd = 0
    
    i=0
    while i<len(n):
        if int(n[i])%2 == 0:
            even = even + int(n[i])
           
        else:
            odd = odd + int(n[i])
        
        i += 1
        

    print(str(even)+" "+str(odd))

sum_even_odd(n)
